<%-include("header")-%> 

    <div class="box" id="heading">
        <h1><%= listHead  %> </h1>
    </div>

    <div class="box">
        <% for(var i =0 ; i< newListItems.length; i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItems[i]  %> </p>
            </div>
        <% } %>
        <form action="/" method="post" class="item">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="listbtn" value="<%=listHead%> ">+</button>
        </form>
    </div>
    <%-include("footer")-%> 